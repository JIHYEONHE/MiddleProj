package reply;

public class ReplyVO {
private int replyNum;
private String dicId;
private int infoboNum;
private String replyCon;
private String replyDate;
private String replyDelCon;



public ReplyVO(int replyNum, String dicId, int infoboNum, String replyCon, String replyDate) {
	this.replyNum = replyNum;
	this.dicId = dicId;
	this.infoboNum = infoboNum;
	this.replyCon = replyCon;
	this.replyDate = replyDate;
	this.replyDelCon = replyDelCon; 
}

public ReplyVO() {
	super();
}


public String getReplyDelCon() {
	return replyDelCon;
}

public void setReplyDelCon(String replyDelCon) {
	this.replyDelCon = replyDelCon;
}

public int getReplyNum() {
	return replyNum;
}
public void setReplyNum(int replyNum) {
	this.replyNum = replyNum;
}
public String getDicId() {
	return dicId;
}
public void setDicId(String dicId) {
	this.dicId = dicId;
}
public int getInfoboNum() {
	return infoboNum;
}
public void setInfoboNum(int infoboNum) {
	this.infoboNum = infoboNum;
}
public String getReplyCon() {
	return replyCon;
}
public void setReplyCon(String replyCon) {
	this.replyCon = replyCon;
}
public String getReplyDate() {
	return replyDate;
}
public void setReplyDate(String replyDate) {
	this.replyDate = replyDate;
}
@Override
public String toString() {
	return "ReplyVO [replyNum=" + replyNum + ", dicId=" + dicId + ", infoboNum=" + infoboNum + ", replyCon=" + replyCon
			+ ", replyDate=" + replyDate + "]";
}
}
